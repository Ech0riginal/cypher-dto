{
  "name": "Cypher DTO",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/cypher-dto",
  "postCreateCommand": "sudo chown -R vscode:vscode /cypher-dto",
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "JScearcy.rust-doc-viewer",
        "tamasfe.even-better-toml",
        "vadimcn.vscode-lldb"
      ]
    }
  }
}
